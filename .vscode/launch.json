  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "restart": true,
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "outFiles": ["${workspaceFolder}/bin/**/*.mjs"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/DigitalLibrary/seeders/20210127204804-populate-users.js"
    }
  ]
}
